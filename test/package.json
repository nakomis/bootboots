{
  "name": "bootboots-cat-detection-test",
  "version": "1.0.0",
  "description": "BootBoots Cat Detection Accuracy Test Suite",
  "main": "cat-detection-accuracy.test.js",
  "scripts": {
    "test": "node cat-detection-accuracy.test.js",
    "test:random": "node cat-detection-accuracy.test.js --random",
